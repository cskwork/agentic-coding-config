{
  "permissions": {
    "allow": [
      "Task",
      "Bash(npm run lint)",
      "Bash(npm run test:*)",
      "Bash(git log:*)",
      "Bash(curl:*)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(ollama:*)",
      "Bash(source:*)",
      "Bash(which:*)",
      "Read(~/.zshrc)",
      "Grep",
      "Read",
      "WebFetch",
      "WebSearch"
    ],
    "deny": [
      "Bash(rm:*)"
    ],
    "defaultMode": "plan"
  },
  "includeCoAuthoredBy": false
}